{
    "manifest_version": 2,
    "name": "MyOwnExtension",
    "version": "1.2",
    "description": "添加百度搜索的右键菜单, GitHub Readme TOC",
    "permissions": [
        "contextMenus",
        "webNavigation", 
        "*://*.github.com/*"
    ],
    "background": {
        "scripts": [
            "src/background.js"
        ]
    },
    "icons": {
        "16": "imgs/icon.png",
        "48": "imgs/icon.png",
        "128": "imgs/icon.png"
    },
    "content_scripts": [
        {
            "css": [
                "style.css"
            ],
            "js": [
                "src/main.js"
            ],
            "matches": [
                "*://*.github.com/*"
            ]
        }
    ]
}
